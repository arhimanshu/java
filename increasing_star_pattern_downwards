import java.util.*;
import java.util.Scanner;//i dont think this is necessary as it is covered in the *
public class increaing_star_pattern_downwards{
    public static void main(String[] args){
        
        Scanner sc=new Scanner(System.in);
        System.out.println("whats the height of pattern u want");
        int n=sc.nextInt();
        for (int i=1; i<=n;i++){
            for (int j=1;j<=i;j++){
                System.out.print("#");
            }System.out.println();
        }
    }
}

