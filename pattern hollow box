import java.util.*;
public class watermelon{
    public static void main(String args[]){
        int n=5;
        int m=6;
        for (i=1;i<=n;i++){
            for (j=1;j<=m;j++){
                if(i==1 || j==1||i==n||j==m){
                    System.out.print("#")
                }
                else{
                   System.oyt.print(" ") 
                }

            }System.out.println()
        }
        
    }
}
